{
  "compilerOptions": {
    "lib": [
      "es2019",
    ],
    "module": "commonjs",
    "strict": true,
    "noImplicitReturns": true,
    "outDir": "lib",
    "sourceMap": true,
    "target": "es6",
    "allowJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@models/*": [
        "./models/*"
      ]
    }
  },
  "compileOnSave": true,
  "include": [
    "./",
  ],
  "ts-node": {
    "require": [
      "tsconfig-paths/register",
      "dotenv/config"
    ],
    "transpileOnly": true
  }
}
